<?xml version="1.0"?>

<beans
	default-destroy-method="destroy"
	default-init-method="afterPropertiesSet"
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
>
	<bean id="com.liferay.hubspot.server.HubSpotServer" class="com.liferay.hubspot.server.HubSpotServerImpl" />
	<bean id="com.liferay.hubspot.server.HubSpotServerUtil" class="com.liferay.hubspot.server.HubSpotServerUtil">
		<property name="hubSpotServer" ref="com.liferay.hubspot.server.HubSpotServer" />
	</bean>
</beans>