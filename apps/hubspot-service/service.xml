<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="com.liferay.hubspot" auto-namespace-tables="false">
	<author>Peter Shin</author>
	<namespace>HubSpot</namespace>
	<entity name="HSContact" local-service="true" remote-service="false">

		<!-- References -->

		<reference package-path="com.liferay.portlet.expando" entity="ExpandoValue" />
	</entity>
	<entity name="HSForm" local-service="true" remote-service="true" />
	<exceptions>
		<exception>HubSpotServer</exception>
		<exception>NoSuchHSContact</exception>
		<exception>NoSuchHSForm</exception>
	</exceptions>
</service-builder>