{namespace MyList}

/**
 * My List
 */
{template .render}
	{@param? items: list<?>}

	<ul>
		<li>This is external!</li>
		{if isNonnull($items)}
			{foreach $item in $items}
				{call MyList.printItem}
					{param item: $item /}
				{/call}
			{/foreach}
		{/if}
	</ul>
{/template}

/**
 * Print list item
 * @param item
 */
{template .printItem}
	<li>{$item}</li>
{/template}